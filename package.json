{
	"name": "base5-ui",
	"version": "1.14.2",
	"description": "5app's reusable UI component library",
	"main": "index.js",
	"scripts": {
		"build-icons": "pixo src-icons/svg --out-dir src/icons --template src-icons/template.js",
		"compile": "babel src --out-dir dist",
		"copypackage": "cp -rf package.json dist",
		"predist": "npm run copypackage",
		"dist": "cd dist && npm publish",
		"docz:dev": "docz dev",
		"docz:build": "docz build",
		"lint": "eslint ./",
		"postpublish": "cp -r dist/package.json .",
		"semantic-release": "semantic-release",
		"test": "npm run lint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/5app/base5-ui.git"
	},
	"author": "Dionysos Dajka",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/5app/base5-ui/issues"
	},
	"homepage": "https://github.com/5app/base5-ui#readme",
	"devDependencies": {
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.0.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/plugin-transform-react-jsx": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@semantic-release/changelog": "^3.0.2",
		"@semantic-release/git": "^7.0.8",
		"babel-eslint": "^10.0.1",
		"babel-plugin-styled-components": "^1.8.0",
		"docz": "^1.0.4",
		"docz-theme-default": "^1.0.4",
		"eslint": "^5.5.0",
		"eslint-config-5app": "^0.3.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.11.1",
		"pixo": "^1.1.2",
		"prop-types": "^15.7.2",
		"react": "^16.8.1",
		"react-dom": "^16.8.1",
		"semantic-release": "^15.13.12",
		"styled-components": "^4.0.1-0"
	},
	"peerDependencies": {
		"react": "^16.8.1",
		"styled-components": "^4.0.1-0"
	},
	"dependencies": {
		"chroma-js": "^1.3.7",
		"classnames": "^2.2.6",
		"prop-types": "^15.7.2",
		"react-hook-size": "^1.3.0",
		"react-popper": "^1.3.3"
	},
	"release": {
		"branch": "master",
		"prepare": [
			"@semantic-release/changelog",
			"@semantic-release/npm",
			{
				"path": "@semantic-release/git",
				"assets": [
					"package.json",
					"CHANGELOG.md"
				],
				"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/npm",
				{
					"pkgRoot": "dist"
				}
			],
			"@semantic-release/github"
		]
	}
}
